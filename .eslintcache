 {...action, ...data}\n    delete finalAction[FETCH_DATA]\n    return finalAction\n  }\n\n  const [requestType, successType, failureType] = types\n\n  next(actionWith({type: requestType}))\n  return fetchData(endpoint, schema).then(\n    response => next(actionWith({\n      type: successType,\n      response \n    })),\n    error => next(actionWith({\n      type: failureType,\n      error: error.message || '获取数据失败'\n    }))\n  )\n}\n\n//执行网络请求\nconst fetchData = (endpoint, schema) => {\n  return get(endpoint).then(data => {\n    return normalizeData(data, schema)\n  })\n}\n\n//根据schema, 将获取的数据扁平化处理\nconst normalizeData = (data, schema) => {\n  const {id, name} = schema\n  let kvObj = {}\n  let ids = []\n  if(Array.isArray(data)) {\n    data.forEach(item => {\n      kvObj[item[id]] = item\n      ids.push(item[id])\n    })\n  } else {\n    kvObj[data[id]] = data\n    ids.push(data[id])\n  }\n  return {\n    [name]: kvObj,\n    ids\n  }\n}\n","D:\\0WorkPlace\\Front_End_iMooc\\300React\\comment-react\\src\\redux\\modules\\index.js",[],"D:\\0WorkPlace\\Front_End_iMooc\\300React\\comment-react\\src\\containers\\Home\\index.js",[],"D:\\0WorkPlace\\Front_End_iMooc\\300React\\comment-react\\src\\utils\\request.js",[],"D:\\0WorkPlace\\Front_End_iMooc\\300React\\comment-react\\src\\redux\\modules\\home.js",[],"D:\\0WorkPlace\\Front_End_iMooc\\300React\\comment-react\\src\\redux\\modules\\detail.js",[],"D:\\0WorkPlace\\Front_End_iMooc\\300React\\comment-react\\src\\redux\\modules\\entities\\index.js",[],"D:\\0WorkPlace\\Front_End_iMooc\\300React\\comment-react\\src\\utils\\url.js",["175"],"D:\\0WorkPlace\\Front_End_iMooc\\300React\\comment-react\\src\\containers\\Home\\components\\Category\\index.js",[],"D:\\0WorkPlace\\Front_End_iMooc\\300React\\comment-react\\src\\redux\\modules\\entities\\shops.js",[],"D:\\0WorkPlace\\Front_End_iMooc\\300React\\comment-react\\src\\redux\\modules\\entities\\orders.js",[],"D:\\0WorkPlace\\Front_End_iMooc\\300React\\comment-react\\src\\redux\\m